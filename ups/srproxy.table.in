FILE=TABLE
PRODUCT=srproxy
VERSION=@SRPROXY_UPS_VERSION@

FLAVOR=ANY
QUALIFIERS=@SRPROXY_UPS_QUALIFIER@

ACTION=SETUP
  setupEnv()
  proddir()
  EnvSet(SRPROXY_VERSION, ${UPS_PROD_VERSION} )
  EnvSet(SRPROXY_INC, ${UPS_PROD_DIR}/include )
  pathPrepend(PATH, ${UPS_PROD_DIR}/bin )
  pathPrepend(LD_LIBRARY_PATH, ${UPS_PROD_DIR}/lib )
  pathPrepend(CMAKE_PREFIX_PATH, ${UPS_PROD_DIR} )

  setupRequired(root @ROOT_UPS_VERSION@ -q @ROOT_UPS_QUALIFIERS@)